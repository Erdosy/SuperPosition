<extension xmlns="http://ns.adobe.com/air/extension/20.0">
      <id>com.KjeMar.LocationExtension</id>
      <versionNumber>1</versionNumber>
      <platforms>
        <platform name="Android-ARM">
          <applicationDeployment>
            <nativeLibrary>AndroidGPSLocationExtension.jar</nativeLibrary>
            <initializer>com.KjeMar.LocationExtension.AndroidGPSLocationExtension</initializer>
          </applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libiOsLocationExtension.a</nativeLibrary>
                <initializer>iOSLocationExtensionExtInitializer</initializer>
            </applicationDeployment>
        </platform>
      </platforms>
</extension>